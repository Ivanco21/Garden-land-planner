<UserControl
    x:Class="WpfClientForm.Views.GargenView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WpfClientForm.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock Text="Сад" />
        <DockPanel LastChildFill="True">
            <TextBlock Text="ж/б концевые 9*9,5 Высота (м)" />
            <TextBox />
        </DockPanel>
        <DockPanel LastChildFill="True">
            <TextBlock Text="ж/б промежуточные 7*7,5 Высота (м)" />
            <TextBox />
        </DockPanel>
        <DockPanel LastChildFill="True">
            <TextBlock Width="145" Text="дерево промежуточные" />
            <TextBlock Width="36" Text="ø(мм)" />
            <TextBox Width="50" />
            <TextBlock Width="58" Text="высота(м)" />
            <TextBox Width="50" />
        </DockPanel>
        <DockPanel LastChildFill="True">
            <TextBlock Width="145" Text="дерево концевые" />
            <TextBlock Width="36" Text="ø(мм)" />
            <TextBox Width="50" />
            <TextBlock Width="58" Text="высота(м)" />
            <TextBox Width="50" />
        </DockPanel>
    </StackPanel>
</UserControl>
